generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model DynamicRole {
  id  String  @id @default(cuid())
  name String @unique
  shortDescription String @db.VarChar(42)
  emojiId String @unique
  amountOfSubscribers Int
}
